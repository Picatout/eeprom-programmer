      000263 B7 20            [ 1]  148     ld PG+GPIO_DDR,a ; output 
      000265 B7 21            [ 1]  149     ld PG+GPIO_CR1,a ; push-pull 
      000267 B7 22            [ 1]  150     ld PG+GPIO_CR2,a ; high speed 
      000269 72 5F 50 0F      [ 1]  151     clr ADDR_LOW  
